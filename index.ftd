-- ftd.rive: 
id: bell
src: $assets.files.bell.riv
autoplay: false
$on-mouse-enter$: $ftd.play-rive(rive = bell, input = Hover)
$on-mouse-leave$: $ftd.pause-rive(rive = bell, input = Hover)

-- ftd.rive:
id: vehicle
src: https://cdn.rive.app/animations/vehicles.riv
autoplay: false
artboard: Jeep
width.fixed.px: 600

-- ftd.text: Idle/Run
$on-click$: $ftd.toggle-play-rive(rive = vehicle, input = idle)
align-self: center


-- ftd.rive:
id: octo
src: $assets.files.octo-loop.riv
autoplay: true
$on-mouse-enter$: $ftd.pause-rive(rive = octo, input = Idle)
$on-mouse-leave$: $ftd.play-rive(rive = octo, input = Idle)


